{
  "name": "household-budget",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --mode development",
    "build:production": "tsc && vite build --mode production",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && pnpm lint:exports",
    "preview": "vite preview",
    "deploy:dev": "pnpm lint && pnpm build && firebase deploy -P development",
    "deploy:prod": "pnpm lint && pnpm build:production && firebase deploy -P production",
    "typecheck": "tsc",
    "cypress": "cypress open",
    "cypress:install": "cypress install",
    "cypress:run": "cypress run",
    "cypress:dev": "CYPRESS_baseUrl=https://household-buget-dev.web.app cypress run",
    "cypress:dev:open": "CYPRESS_baseUrl=https://household-buget-dev.web.app cypress open",
    "lint:exports": "node find-unused-exports.cjs",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "firebase": "12.5.0",
    "nanoid": "5.0.6",
    "react": "18.3.1",
    "react-datepicker": "7.5.0",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.1.2",
    "react-firebase-hooks": "5.1.1",
    "react-router-dom": "6.28.0",
    "recharts": "^2.13.3",
    "styled-components": "6.1.19"
  },
  "devDependencies": {
    "@testing-library/cypress": "10.1.0",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "14.6.1",
    "@types/node": "22.9.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.35",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vitejs/plugin-react-swc": "4.2.0",
    "cypress": "13.6.6",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "patch-package": "8.0.1",
    "ts-unused-exports": "10.1.0",
    "typescript": "5.9.3",
    "vite": "5.4.11",
    "vite-tsconfig-paths": "5.1.4"
  }
}
